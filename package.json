{
  "name": "dungeon-delvers-metadata-server",
  "version": "2.0.1",
  "description": "Static JSON-based metadata server for Dungeon Delvers NFTs",
  "main": "src/index.js",
  "scripts": {
    "start": "node src/index.js",
    "dev": "nodemon src/index.js",
    "test:config": "node test-config.js",
    "generate:static": "node scripts/generate-existing-nfts.js",
    "generate:heroes": "node scripts/generate-existing-nfts.js --type=hero",
    "generate:relics": "node scripts/generate-existing-nfts.js --type=relic",
    "generate:parties": "node scripts/generate-existing-nfts.js --type=party",
    "static:health": "curl -s http://localhost:3000/api/static/health | jq ."
  },
  "dependencies": {
    "axios": "^1.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "ethers": "^6.15.0",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "morgan": "^1.10.0",
    "node-cache": "^5.1.2",
    "rate-limiter-flexible": "^4.0.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
